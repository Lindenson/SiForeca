docker container inspect sf-redis > /dev/null || docker run --name sf-redis -p 6379:6379 -d redis:6.0 redis-server --requirepass "danlee"
docker container inspect sf-postgres > /dev/null || docker run --name sf-postgres -e POSTGRES_PASSWORD=misha -d -p 5432:5432 postgres
docker container inspect sf-postgres > /dev/null || cat ./my.sql | docker exec -i sf-postgres psql -U postgres

docker container start sf-postgres
docker container start sf-redis

mvn test -Drun.security.tests=true

docker container stop sf-postgres
docker container stop sf-redis



